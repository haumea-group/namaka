		"renraku": "^0.0.0-dev.58"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@typescript-eslint/eslint-plugin": "^5.22.0",
		"@typescript-eslint/parser": "^5.22.0",
		"cynic": "0.1.0-dev.16",
		"eslint": "^8.14.0",
		"http-server": "^14.1.0",
